<?xml version="1.0" encoding="UTF-8"?>

<wsdl:description xmlns:wsdl="http://www.w3.org/ns/wsdl"
        targetNamespace="http://www.interoud.com/vodkatv/wsdl"
        xmlns:tns="http://www.interoud.com/vodkatv/wsdl"
        xmlns:whttp="http://www.w3.org/ns/wsdl/http"
        xmlns:wsdlx="http://www.w3.org/ns/wsdl-extensions"
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns:msg="http://www.interoud.com/vodkatv/xsd">

    <wsdl:documentation>
    WSDL for VoDKATV OSS/BSS API
    </wsdl:documentation>

    <!--
    Types
    -->
    <wsdl:types>
        <xs:import namespace="http://www.interoud.com/vodkatv/xsd"
            schemaLocation="vodkatv.xsd"/>
    </wsdl:types>

    <!--
    Interface
    -->
    <wsdl:interface name="VoDKATVInterface">

        <!-- Room -->
        <wsdl:operation name="CreateRoom"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It creates a new household with the specified values.
            </wsdl:documentation>

            <wsdl:input element="msg:createRoomParams"/>
            <wsdl:output element="msg:createRoomResponse"/>
        </wsdl:operation>

        <wsdl:operation name="FindAllRooms"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns all households already created in the system.
            </wsdl:documentation>

            <wsdl:output element="msg:rooms"/>
        </wsdl:operation>

        <wsdl:operation name="DeleteRoom"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It deletes an existing household and all their associated devices
            from the system.
            </wsdl:documentation>

            <wsdl:input element="msg:deleteRoomParams"/>
            <wsdl:output element="msg:deleteRoomResponse"/>
        </wsdl:operation>

        <!-- Device -->
        <wsdl:operation name="CreateDevice"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It creates a new device with the specified values.
            </wsdl:documentation>

            <wsdl:input element="msg:createDeviceParams"/>
            <wsdl:output element="msg:createDeviceResponse"/>
        </wsdl:operation>

        <wsdl:operation name="FindDeviceById"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the device with the given identifier.
            </wsdl:documentation>

            <wsdl:input element="msg:findDeviceByIdParams"/>
            <wsdl:output element="msg:findDeviceByIdResponse"/>
        </wsdl:operation>

        <wsdl:operation name="FindDevices"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the list of devices filtering by a given criteria.
            </wsdl:documentation>

            <wsdl:input element="msg:findDevicesParams"/>
            <wsdl:output element="msg:findDevicesResponse"/>
        </wsdl:operation>

        <wsdl:operation name="FindDevicesByRoom"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns a list of the devices from the specified household.
            </wsdl:documentation>

            <wsdl:input element="msg:findDevicesByRoomParams"/>
            <wsdl:output element="msg:findDevicesByRoomResponse"/>
        </wsdl:operation>

        <wsdl:operation name="FindDevicesByTagId"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the list of the devices with the specified tag node.
            </wsdl:documentation>

            <wsdl:input element="msg:findDevicesByTagIdParams"/>
            <wsdl:output element="msg:findDevicesByTagIdResponse"/>
        </wsdl:operation>

        <wsdl:operation name="UpdateDevice"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It updates the information associated to an already existing device.
            </wsdl:documentation>

            <wsdl:input element="msg:updateDeviceParams"/>
            <wsdl:output element="msg:updateDeviceResponse"/>
        </wsdl:operation>

        <wsdl:operation name="DeleteDevice"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It deletes an existing device from the system.
            </wsdl:documentation>

            <wsdl:input element="msg:deleteDeviceParams"/>
            <wsdl:output element="msg:deleteDeviceResponse"/>
        </wsdl:operation>

        <!-- DeviceClasses -->
        <wsdl:operation name="FindAllDeviceClasses"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns all the device classes available in the system.
            </wsdl:documentation>

            <wsdl:output element="msg:findAllDeviceClassesResponse"/>
        </wsdl:operation>

        <!-- Session -->
        <wsdl:operation name="CreateSession"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It creates a new session in an existing household. This operation
            will also create a default account for the created session.
            </wsdl:documentation>

            <wsdl:input element="msg:createSessionParams"/>
            <wsdl:output element="msg:createSessionResponse"/>
        </wsdl:operation>

        <wsdl:operation name="FindSessionById"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the session with the given identifier.
            </wsdl:documentation>

            <wsdl:input element="msg:findSessionByIdParams"/>
            <wsdl:output element="msg:findSessionByIdResponse"/>
        </wsdl:operation>

        <wsdl:operation name="FindSessionsByRoom"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns a list of the sessions created in the specified household.
            </wsdl:documentation>

            <wsdl:input element="msg:findSessionsByRoomParams"/>
            <wsdl:output element="msg:findSessionsByRoomResponse"/>
        </wsdl:operation>

        <wsdl:operation name="UpdateSession"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It updates the information associated to an already existing session.
            </wsdl:documentation>

            <wsdl:input element="msg:updateSessionParams"/>
            <wsdl:output element="msg:updateSessionResponse"/>
        </wsdl:operation>

        <wsdl:operation name="DeleteSession"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It deletes an existing session and all associated accounts from the system.
            </wsdl:documentation>

            <wsdl:input element="msg:deleteSessionParams"/>
            <wsdl:output element="msg:deleteSessionResponse"/>
        </wsdl:operation>

        <!-- Account -->
        <wsdl:operation name="CreateAccount"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It creates a new account for a session. This operation will not
            associate any login/password to access the system. On the other
            hand, the value for outputsProtection} and parentalControlLevel}
            will be the default values defined on the VoDKATV system.
            </wsdl:documentation>

            <wsdl:input element="msg:createAccountParams"/>
            <wsdl:output element="msg:createAccountResponse"/>
        </wsdl:operation>

        <wsdl:operation name="FindAccountsBySessionAccounting"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the accounts defined for the specified session.
            </wsdl:documentation>

            <wsdl:input element="msg:findAccountsBySessionAccountingParams"/>
            <wsdl:output element="msg:findAccountsBySessionAccountingResponse"/>
        </wsdl:operation>

        <wsdl:operation name="UpdateAccount"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It updates the information associated to an already existing account.
            </wsdl:documentation>

            <wsdl:input element="msg:updateAccountParams"/>
            <wsdl:output element="msg:updateAccountResponse"/>
        </wsdl:operation>

        <wsdl:operation name="DeleteAccount"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It updates the information associated to an already existing account.
            </wsdl:documentation>

            <wsdl:input element="msg:deleteAccountParams"/>
            <wsdl:output element="msg:deleteAccountResponse"/>
        </wsdl:operation>

        <!-- AccountDetails -->
        <wsdl:operation name="CreateAccountDetails"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It creates a new account for a session.
            </wsdl:documentation>

            <wsdl:input element="msg:createAccountDetailsParams"/>
            <wsdl:output element="msg:createAccountDetailsResponse"/>
        </wsdl:operation>

        <wsdl:operation name="FindAccountDetailsById"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the accounts details for a given account identifier.
            </wsdl:documentation>

            <wsdl:input element="msg:findAccountDetailsByIdParams"/>
            <wsdl:output element="msg:findAccountDetailsByIdResponse"/>
        </wsdl:operation>

        <wsdl:operation name="FindAccountsDetailsBySessionAccounting"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the accounts details defined for the specified session.
            </wsdl:documentation>

            <wsdl:input element="msg:findAccountsDetailsBySessionAccountingParams"/>
            <wsdl:output element="msg:findAccountsDetailsBySessionAccountingResponse"/>
        </wsdl:operation>

        <wsdl:operation name="UpdateAccountDetails"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It updates the information associated to an already existing account.
            </wsdl:documentation>

            <wsdl:input element="msg:updateAccountDetailsParams"/>
            <wsdl:output element="msg:updateAccountDetailsResponse"/>
        </wsdl:operation>

        <!-- RoomDeviceSession -->
        <wsdl:operation name="CreateRoomDeviceSession"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It creates a new household, and associates the specified devices to
            that household. Also, it creates a session associated to that
            household.
            </wsdl:documentation>

            <wsdl:input element="msg:createRoomDeviceSessionParams"/>
            <wsdl:output element="msg:createRoomDeviceSessionResponse"/>
        </wsdl:operation>

        <wsdl:operation name="FindRoomDeviceSessionById"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the household with their associated devices and current
            session with the given session identifier.
            </wsdl:documentation>

            <wsdl:input element="msg:findRoomDeviceSessionByIdParams"/>
            <wsdl:output element="msg:findRoomDeviceSessionByIdResponse"/>
        </wsdl:operation>

        <wsdl:operation name="FindRoomDeviceSessions"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns a list of the households with the associated sessions and
            devices.
            </wsdl:documentation>

            <wsdl:input element="msg:findRoomDeviceSessionsParams"/>
            <wsdl:output element="msg:findRoomDeviceSessionsResponse"/>
        </wsdl:operation>

        <wsdl:operation name="FindRoomDeviceSessionsByTagId"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the list of households with the associated sessions and
            devices from the specified tag.
            </wsdl:documentation>

            <wsdl:input element="msg:findRoomDeviceSessionsByTagIdParams"/>
            <wsdl:output element="msg:findRoomDeviceSessionsByTagIdResponse"/>
        </wsdl:operation>

        <wsdl:operation name="UpdateRoomDeviceSession"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It updates the information associated to an already existing household.
            </wsdl:documentation>

            <wsdl:input element="msg:updateRoomDeviceSessionParams"/>
            <wsdl:output element="msg:updateRoomDeviceSessionResponse"/>
        </wsdl:operation>

        <wsdl:operation name="DeleteRoomDeviceSession"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It deletes an existing household and its associated devices and
            sessions from the system.
            </wsdl:documentation>

            <wsdl:input element="msg:deleteRoomDeviceSessionParams"/>
            <wsdl:output element="msg:deleteRoomDeviceSessionResponse"/>
        </wsdl:operation>

        <!-- VideoServer -->
        <wsdl:operation name="CreateVideoServer"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It creates a new video server on the system.
            </wsdl:documentation>

            <wsdl:input element="msg:updateOrCreateVideoServerParams"/>
            <wsdl:output element="msg:videoServer"/>
        </wsdl:operation>

        <wsdl:operation name="FindVideoServerById"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the data of a video server created on the system.
            </wsdl:documentation>

            <wsdl:input element="msg:findVideoServerById"/>
            <wsdl:output element="msg:videoServer"/>
        </wsdl:operation>

        <wsdl:operation name="FindAllVideoServers"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the list of video servers created on the system.
            </wsdl:documentation>

            <wsdl:output element="msg:videoServers"/>
        </wsdl:operation>

        <wsdl:operation name="UpdateVideoServer"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It updates an already existing video server.
            </wsdl:documentation>

            <wsdl:input element="msg:updateOrCreateVideoServerParams"/>
            <wsdl:output element="msg:videoServer"/>
        </wsdl:operation>

        <wsdl:operation name="DeleteVideoServer"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It deletes a video server.
            </wsdl:documentation>

            <wsdl:input element="msg:deleteVideoServer"/>
            <wsdl:output element="msg:videoServers"/>
        </wsdl:operation>

        <wsdl:operation name="UpdateVideoServerStatus"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It updates the status of the video servers.
            </wsdl:documentation>

            <wsdl:input element="msg:updateOrCreateVideoServerStatusParams"/>
            <wsdl:output element="msg:videoServerStatus"/>
        </wsdl:operation>

        <wsdl:operation name="EnableVideoServer"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            Enable a video server.
            </wsdl:documentation>

            <wsdl:input element="msg:findVideoServerById"/>
            <wsdl:output element="msg:videoServer"/>
        </wsdl:operation>

        <wsdl:operation name="DisableVideoServer"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            Disable a video server.
            </wsdl:documentation>

            <wsdl:input element="msg:findVideoServerById"/>
            <wsdl:output element="msg:videoServer"/>
        </wsdl:operation>

        <wsdl:operation name="CreateVideoServerStatus"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It creates a new video server status on the system.
            </wsdl:documentation>

            <wsdl:input element="msg:updateOrCreateVideoServerStatusParams"/>
            <wsdl:output element="msg:videoServerStatus"/>
        </wsdl:operation>

        <wsdl:operation name="FindVideoServerStatusById"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the data of a video server status created on the system.
            </wsdl:documentation>

            <wsdl:input element="msg:findVideoServerStatusById"/>
            <wsdl:output element="msg:videoServerStatus"/>
        </wsdl:operation>

        <wsdl:operation name="FindAllVideoServersStatus"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the list of video servers status created on the system.
            </wsdl:documentation>

            <wsdl:output element="msg:videoServersStatus"/>
        </wsdl:operation>

        <wsdl:operation name="DeleteVideoServerStatus"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It deletes a video server status.
            </wsdl:documentation>

            <wsdl:input element="msg:deleteVideoServerStatus"/>
            <wsdl:output element="msg:videoServersStatus"/>
        </wsdl:operation>

        <!-- VideoServerGroup -->
        <wsdl:operation name="CreateVideoServerGroup"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It creates a new video server group on the system.
            </wsdl:documentation>

            <wsdl:input element="msg:updateOrCreateVideoServerGroupParams"/>
            <wsdl:output element="msg:videoServerGroup"/>
        </wsdl:operation>

        <wsdl:operation name="FindVideoServerGroupById"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the data of a video server group created on the system.
            </wsdl:documentation>

            <wsdl:input element="msg:findVideoServerGroupById"/>
            <wsdl:output element="msg:videoServerGroup"/>
        </wsdl:operation>

        <wsdl:operation name="FindVideoServerGroupByType"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the list of a video server group created on the system with the given type.
            </wsdl:documentation>

            <wsdl:input element="msg:findVideoServerGroupByType"/>
            <wsdl:output element="msg:videoServerGroups"/>
        </wsdl:operation>

        <wsdl:operation name="FindAllVideoServerGroups"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the list of video server groups created on the system.
            </wsdl:documentation>

            <wsdl:output element="msg:videoServerGroups"/>
        </wsdl:operation>

        <wsdl:operation name="UpdateVideoServerGroup"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It updates an already existing video server group.
            </wsdl:documentation>

            <wsdl:input element="msg:updateOrCreateVideoServerGroupParams"/>
            <wsdl:output element="msg:videoServerGroup"/>
        </wsdl:operation>

        <wsdl:operation name="DeleteVideoServerGroup"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It deletes a video server group.
            </wsdl:documentation>

            <wsdl:input element="msg:deleteVideoServerGroup"/>
            <wsdl:output element="msg:videoServerGroups"/>
        </wsdl:operation>


        <!-- ScreenSaver -->
        <wsdl:operation name="CreateScreenSaver"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It creates a new screenSaver
            </wsdl:documentation>

            <wsdl:input element="msg:updateOrCreatescreenSaverParams"/>
            <wsdl:output element="msg:screenSaver"/>
        </wsdl:operation>

        <wsdl:operation name="FindScreenSaverById"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the screenSaver by given identifier.
            </wsdl:documentation>

            <wsdl:input element="msg:findScreenSaverByIdParams"/>
            <wsdl:output element="msg:screenSaver"/>
        </wsdl:operation>

        <wsdl:operation name="FindScreenSavers"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns a list of the screenSavers
            </wsdl:documentation>

            <wsdl:input element="msg:findScreenSaversParams"/>
            <wsdl:output element="msg:screenSavers"/>
        </wsdl:operation>

        <wsdl:operation name="UpdateScreenSaver"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It updates the information associated to an already existing screenSaver.
            </wsdl:documentation>

            <wsdl:input element="msg:updateOrCreatescreenSaverParams"/>
            <wsdl:output element="msg:screenSaver"/>
        </wsdl:operation>

        <wsdl:operation name="DeleteScreenSaver"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It deletes an existing screenSaver
